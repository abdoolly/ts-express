{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-plugin-prettier"
  ],
  "jsRules": {
    "no-unused-expression": true
  },
  "rules": {
    "prettier": [
      false,
      {
        "singleQuote": true
      }
    ],
    "quotemark": [
      true,
      "single"
    ],
    "member-access": [
      false
    ],
    "ordered-imports": [
      false
    ],
    "max-line-length": false,
    "member-ordering": [
      false
    ],
    "interface-name": [
      false
    ],
    "arrow-parens": false,
    "object-literal-sort-keys": false,
    "trailing-comma": false,
    "await-promise": true,
    "no-console": false,
    "only-arrow-functions": false,
    "no-require-imports": false,
    "no-var-requires": false,
    "prefer-const": false,
    "indent": true,
    "curly": false,
    "ban-types": false,
    "space-before-function-paren": false,
    "no-empty-interface": false
  },
  "rulesDirectory": []
}